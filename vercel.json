{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|webp|gif|pdf|json))$",
      "headers": {
        "cache-control": "max-age=31536000"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
